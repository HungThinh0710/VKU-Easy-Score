{
  "name": "VKU Easy Score",
  "description": "Một tiện ích trình duyệt giúp vượt qua 1 nghìn 900 tỷ thao tác lúc đánh giá để xem điểm số của hệ thống Vê Ca Du.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./icons/logo-16x16.png",
    "32": "./icons/logo-32x32.png",
    "48": "./icons/logo-48x48.png",
    "128": "./icons/logo-128x128.png"
  },
  "background": {
    "service_worker": "wrapper.js"
  },
  "action": {},
  "permissions": [
    "tabs",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "http://daotao.vku.udn.vn/*"
  ],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "http://daotao.vku.udn.vn/*"
      ],
      "all_frames": true,
      "js": [
        "jquery.min.js",
        "content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "test3.png",
        "test4.png"
      ],
      "matches": [
        "http://daotao.vku.udn.vn/*"
      ],
      "use_dynamic_url": true
    }
  ]
}